   "source": [
    "def removeDuplicates(self, nums: List[int]) -> int:\n",
    "        l = 1\n",
    "        \n",
    "        for r in range(l, len(nums)) :\n",
    "            if nums[r] != nums[ r - 1] :\n",
    "                nums[l] = nums[r]\n",
    "                l += 1\n",
    "        return l"
   ]
  }
